{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "Used to express the dynamic constraints on TCP/RTP sender transport parameters. These constraints may be set and changed at run time. Every transport parameter must have an entry, even if it is only an empty object.",
  "required": [
    "source_ip",
    "source_port",
    "rtp_enabled"
  ],
  "additionalProperties": false,
  "patternProperties": {
    "^ext_[a-zA-Z0-9_]+$":{
      "$ref": "constraint-schema.json#/definitions/constraint"
    }
  },
  "properties": {
    "source_ip": {
      "$ref": "constraint-schema.json#/definitions/constraint"
    },
    "source_port": {
      "$ref": "constraint-schema.json#/definitions/constraint"
    },
    "rtcp_enabled": {
      "$ref": "constraint-schema.json#/definitions/constraint"
    },
    "rtcp_source_port": {
      "$ref": "constraint-schema.json#/definitions/constraint"
    },
    "rtp_enabled": {
      "$ref": "constraint-schema.json#/definitions/constraint"
    }
  }
}
